<h1>New url_item_comment</h1>

<form id="new-url_item_comment" name="url_item_comment">
  <div class="field">
    <label for="comment"> comment:</label>
    <input type="text" name="comment" id="comment" value="<%= comment %>" >
  </div>

  <div class="field">
    <label for="url_item_id"> url_item_id:</label>
    <input type="text" name="url_item_id" id="url_item_id" value="<%= url_item_id %>" >
  </div>

  <div class="actions">
    <input type="submit" value="Create Url item comment" />
  </div>

</form>

